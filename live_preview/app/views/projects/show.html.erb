<%= tag.h1 @project.name %>

<hr>

<% if @project.url.present? %>
	<%= content_tag :iframe, '', src: @project.url, width: "100%", height: 600 %>
<% else %>
	<%= tag.p "This project has not been uploaded yet." %>
<% end %>

<% if @project.source.present? %>
	<%= link_to "Source", @project.source %>
<% else %>
	<%= tag.p "Source code unavailable." %>
<% end %>

<% if @project.notes.present? %>
	<%= tag.p @project.notes %>
<% end %>